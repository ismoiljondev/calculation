    "schemas": {
      "CreateExchangeRateDto": {
        "type": "object",
        "properties": {
          "rate": { "type": "number" },
          "bank_name": { "type": "string" }
        },
        "required": ["rate", "bank_name"]
      },
      "PaginationMeta": {
        "type": "object",
        "properties": {
          "total": { "type": "number" },
          "page": { "type": "number" },
          "limit": { "type": "number" },
          "totalPages": { "type": "number" }
        },
        "required": ["total", "page", "limit", "totalPages"]
      },
      "CreateExchangeRateDtoPaginatedResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/CreateExchangeRateDto" }
          },
          "meta": { "$ref": "#/components/schemas/PaginationMeta" }
        },
        "required": ["data", "meta"]
      },
      "UpdateExchangeRateDto": {
        "type": "object",
        "properties": {
          "rate": { "type": "number" },
          "bank_name": { "type": "string" }
        }
      },
      "CreateProductTypeDto": {
        "type": "object",
        "properties": { "name": { "type": "string" } },
        "required": ["name"]
      },
      "CreateProductTypeDtoPaginatedResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/CreateProductTypeDto" }
          },
          "meta": { "$ref": "#/components/schemas/PaginationMeta" }
        },
        "required": ["data", "meta"]
      },
      "UpdateProductTypeDto": {
        "type": "object",
        "properties": { "name": { "type": "string" } }
      },
      "Unit": {
        "type": "string",
        "enum": ["PIECE", "METER", "SQUAREMETER", "KILOGRAMM"]
      },
      "CreateProductVariantDto": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "type": { "type": "string" },
          "unit": { "allOf": [{ "$ref": "#/components/schemas/Unit" }] }
        },
        "required": ["name", "type", "unit"]
      },
      "CreateProductVariantDtoPaginatedResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/CreateProductVariantDto" }
          },
          "meta": { "$ref": "#/components/schemas/PaginationMeta" }
        },
        "required": ["data", "meta"]
      },
      "UpdateProductVariantDto": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "type": { "type": "string" },
          "unit": { "allOf": [{ "$ref": "#/components/schemas/Unit" }] }
        }
      },
      "CreateContactDto": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "phone": { "type": "string" }
        },
        "required": ["name", "phone"]
      },
      "CreateContactDtoPaginatedResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/CreateContactDto" }
          },
          "meta": { "$ref": "#/components/schemas/PaginationMeta" }
        },
